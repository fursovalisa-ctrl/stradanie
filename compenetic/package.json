{
  "name": "skillgrid",
  "version": "0.0.21",
  "description": "Skillgrid Components Library",
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "author": "Skillaz",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": "https://gitlab.dev.skillaz.ru/frontend/skillgrid",
  "homepage": "https://skillaz.ru/",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "tsc",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:css": "stylelint **/*.css",
    "lint:css:fix": "stylelint **/*.css --fix",
    "lint:fix:all": "npm run lint:fix && npm run lint:css:fix",
    "preview": "vite preview",
    "skillgrid": "tsx scripts/generators/index.ts",
    "test:generators": "vitest run scripts/generators/tests",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "start": "concurrently \"yarn storybook\" \"yarn test:watch\" --kill-others --names \"STORYBOOK,TESTS\" --prefix-colors \"magenta.bold,green.bold\" --prefix \"[{name}]\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs": "yarn docs:dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.17",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^8.2.2",
    "cosmiconfig": "^9.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.0.0",
    "husky": "^9.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "^8.6.12",
    "stylelint": "^15.11.0",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitepress": "^1.0.0-rc.44",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "17.0.2 || 18.3.1",
    "react-dom": "17.0.2 || 18.3.1"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "dependencies": {
    "@testing-library/react": "^16.3.0",
    "antd": "^5.25.1",
    "chalk": "^5.4.1",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "prettier": "^3.5.3",
    "react-i18next": "^15.5.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "resolutions": {
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "wrap-ansi": "7.0.0",
    "cliui": "7.0.4"
  }
}
